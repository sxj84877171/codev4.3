<resources>
	<style name="PviProgressBarStyle" parent="@android:style/Widget.ProgressBar.Horizontal">

		<item name="android:indeterminateOnly">false</item>
		<item name="android:progressDrawable">@drawable/pvi_progress_horizontal</item>
		<item name="android:indeterminateDrawable">@drawable/pvi_progress_indeterminate_horizontal</item>
		<item name="android:minHeight">25dip</item>
		<item name="android:maxHeight">25dip</item>

	</style>

	<style name="TextAppearance2">
		<item name="android:textColor">?textColorPrimary</item>
		<item name="android:textColorHighlight">#FFFF9200</item>
		<item name="android:textColorHint">?textColorHint</item>
		<item name="android:textColorLink">#5C5CFF</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textStyle">normal</item>
	</style>

	<style name="TextAppearance2.DialogWindowTitle2">
		<item name="android:textSize">18sp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:textColor">?textColorPrimary</item>
	</style>

	<style name="DialogWindowTitle2">
		<item name="android:maxLines">1</item>
		<item name="android:scrollHorizontally">true</item>
		<item name="android:textAppearance">@style/TextAppearance2.DialogWindowTitle2</item>
	</style>

	<!-- 无动画 -->
	<style name="Animation.Null" parent="@android:style/Animation">
		<item name="android:windowEnterAnimation">@anim/dialog_null</item>
		<item name="android:windowExitAnimation">@anim/dialog_null</item>
	</style>

	<!-- 窗口展开动画效果 -->
	<style name="Animation.Ani1" parent="@android:style/Animation">
		<item name="android:windowEnterAnimation">@anim/pvialertdialog_enter</item>
		<item name="android:windowExitAnimation">@anim/pvialertdialog_exit</item>
	</style>

	<style name="Theme.Dialog.FontSelect" parent="@android:style/Theme.Dialog">
		<item name="android:backgroundDimEnabled">false</item>
	</style>

	<style name="myannotation_1">
		<item name="android:textColor">@color/black</item>
		<item name="android:textSize">21sp</item>
		<item name="android:layout_width">239px</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">28px</item>
		<item name="android:layout_marginTop">25px</item>
	</style>

	<style name="myannotation_2">
		<item name="android:textColor">@color/black</item>
		<item name="android:textSize">15sp</item>
		<item name="android:layout_width">239px</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">28px</item>
		<item name="android:layout_marginBottom">10px</item>

	</style>

	<style name="myannotation_3">
		<item name="android:textColor">@color/black</item>
		<item name="android:textSize">15sp</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginRight">10dp</item>
	</style>

	<style name="myannotation_4">
		<item name="android:layout_marginLeft">28px</item>
	</style>

	<style name="translucent">
		<item name="android:windowIsTranslucent">true</item>

		<item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>

	</style>

	<!-- Pvi   AlertDialog-->
	<style name="AlertDialog">
		<item name="fullDark">@drawable/popup_full_dark_pviapp</item>
		<item name="topDark">@drawable/popup_top_dark_pviapp</item>
		<item name="centerDark">@drawable/popup_center_dark_pviapp</item>
		<item name="bottomDark">@drawable/popup_bottom_dark_pviapp</item>
		<item name="fullBright">@drawable/popup_full_bright_pviapp</item>
		<item name="topBright">@drawable/popup_top_bright_pviapp</item>
		<item name="centerBright">@drawable/popup_center_bright_pviapp</item>
		<item name="bottomBright">@drawable/popup_bottom_bright_pviapp</item>
		<item name="bottomMedium">@drawable/popup_bottom_medium_pviapp</item>
		<item name="centerMedium">@drawable/popup_center_medium_pviapp</item>
	</style>

	<style name="CheckBoxPviUi1" parent="@android:style/Widget.CompoundButton">
		<item name="android:button">@drawable/btn_check_pvi_ui1</item>
	</style>

	<style name="round_edittext" parent="@android:style/Widget.EditText">
		<item name="android:focusable">true</item>
		<item name="android:focusableInTouchMode">true</item>
		<item name="android:clickable">true</item>
		<item name="android:background">@drawable/edit_input</item>
		<item name="android:paddingLeft">10dp</item>
	</style>
</resources>