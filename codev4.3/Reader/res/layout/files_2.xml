<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" 
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@color/white"
	android:gravity="center_horizontal"
	>
	
 <LinearLayout android:layout_width="wrap_content" android:orientation="horizontal" android:layout_marginRight="25dp" android:layout_marginBottom="25dp"
					  android:gravity="center_horizontal"	android:layout_height="wrap_content">
		<Button
			android:id="@+id/select_3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:background="@drawable/notcheck"
		/>
		<TextView android:text="5 " android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:gravity="center_vertical"
				  android:textColor="@color/black"
				  android:textSize="22px"
				   android:layout_marginLeft="10dp"
				   android:id="@+id/time_3"
				  />
		</LinearLayout>
		
		<LinearLayout android:layout_width="wrap_content" android:orientation="horizontal" android:layout_marginRight="25dp" android:layout_marginBottom="25dp"
					  android:gravity="center_horizontal"	android:layout_height="wrap_content">
		<Button
			android:id="@+id/select_5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:background="@drawable/notcheck"
		/>
		<TextView android:text="30 " android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:gravity="center_vertical"
				  android:textColor="@color/black"
				  android:textSize="22px"
				   android:layout_marginLeft="10dp"
				   android:id="@+id/time_5"
				  />
		</LinearLayout>
		<LinearLayout android:layout_width="wrap_content" android:orientation="horizontal"  android:layout_marginRight="25dp" android:layout_marginBottom="25dp"
					  android:gravity="center_horizontal"	android:layout_height="wrap_content">
		<Button
			android:id="@+id/select_8"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:background="@drawable/notcheck"
		/>
		<TextView android:text="60 " android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:gravity="center_vertical"
				  android:textColor="@color/black"
				  android:textSize="22px"
				   android:layout_marginLeft="10dp"
				   android:id="@+id/time_8"
				  />
		</LinearLayout>
		<LinearLayout android:layout_width="wrap_content" android:orientation="horizontal"  android:layout_marginBottom="25dp"
					  android:gravity="center_horizontal"	android:layout_height="wrap_content">
		<Button
			android:id="@+id/select_10"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:background="@drawable/notcheck"
		/>
		<TextView android:text="90 " android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:gravity="center_vertical"
				  android:textColor="@color/black"
				  android:textSize="22px"
				   android:layout_marginLeft="10dp"
				   android:id="@+id/time_10"
				  />
		</LinearLayout>
</LinearLayout>