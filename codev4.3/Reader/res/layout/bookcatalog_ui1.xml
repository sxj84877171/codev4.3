			<ImageView android:background="@drawable/listbackground_ui1"
				android:layout_x="0dp" android:duplicateParentState="true"
				android:layout_width="fill_parent" android:scaleType="center" android:layout_height="3dp" android:layout_y="80dp"></ImageView>
		</AbsoluteLayout>

		<!--item8-->
		<AbsoluteLayout android:layout_width="fill_parent"
			android:focusableInTouchMode="true" android:clickable="true"
			android:id="@+id/chapter08layout" android:focusable="true" android:layout_height="83dp">
			<TextView android:id="@+id/chapter08" android:layout_width="450dp"
				android:layout_height="82dp" android:text="" android:gravity="center_vertical"
				android:duplicateParentState="true" android:layout_y="0dp"
				android:singleLine="true" android:textAppearance="@style/normal.black.big"
				android:layout_x="10dp">
			</TextView>
			<TextView android:id="@+id/fee08" android:layout_width="70dp"
				android:duplicateParentState="true" android:layout_height="82dp"
				android:text="" android:gravity="center_vertical" android:layout_x="500dp"
				android:layout_y="0dp" android:textAppearance="@style/normal.black.common">
			</TextView>
			<ImageView android:background="@drawable/listbackground_ui1"
				android:layout_x="0dp" android:duplicateParentState="true"
				android:layout_width="fill_parent" android:scaleType="center" android:layout_height="3dp" android:layout_y="80dp"></ImageView>
		</AbsoluteLayout>
	</LinearLayout>

</RelativeLayout>


