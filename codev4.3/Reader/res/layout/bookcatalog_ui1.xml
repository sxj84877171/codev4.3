			</TextView>
			<ImageView android:background="@drawable/listbackground_ui1"
				android:layout_x="0dp" android:duplicateParentState="true"
				android:layout_width="fill_parent" android:scaleType="center" android:layout_height="3dp" android:layout_y="80dp"></ImageView>
		</AbsoluteLayout>

		<!--item5-->
		<AbsoluteLayout android:layout_width="fill_parent"
			android:clickable="true" android:id="@+id/chapter05layout"
			android:focusableInTouchMode="true" android:focusable="true" android:layout_height="83dp">
