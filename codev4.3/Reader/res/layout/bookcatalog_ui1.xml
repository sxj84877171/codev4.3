		</AbsoluteLayout>
		<!--item6-->
		<AbsoluteLayout android:layout_width="fill_parent"
			android:focusableInTouchMode="true" android:clickable="true"
			android:id="@+id/chapter06layout" android:focusable="true" android:layout_height="83dp">
			<TextView android:id="@+id/chapter06" android:layout_width="450dp"
				android:layout_height="82dp" android:text=""
				android:duplicateParentState="true" android:gravity="center_vertical"
				android:layout_y="0dp" android:textAppearance="@style/normal.black.big"
				android:singleLine="true" android:layout_x="10dp">
			</TextView>
			<TextView android:id="@+id/fee06" android:layout_width="70dp"
				android:duplicateParentState="true" android:layout_height="82dp"
