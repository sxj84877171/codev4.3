<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#ffffff"
	android:id="@+id/nosdcard">
	<LinearLayout android:id="@+id/layout01" android:layout_height="wrap_content" 
	android:layout_width="wrap_content" android:layout_centerHorizontal="true"
	android:layout_centerVertical="true"  >
	<ImageView android:id="@+id/alertpic" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:src="@drawable/alertpic"
		  />
	<TextView android:id="@+id/alertmsg" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_toRightOf="@+id/alertpic"
		android:layout_alignTop="@+id/alertpic" android:layout_marginLeft="30dp"
		android:text="@string/myimageSDnone" android:textAppearance="@style/normal.black.big">

	</TextView>
	
	</LinearLayout>
	<Button android:id="@+id/returnback" android:layout_width="wrap_content"  android:layout_height="wrap_content"
	        style="@style/button_ui1" android:text="返回首页" android:layout_below="@+id/layout01" 
	        android:layout_centerHorizontal="true" android:layout_marginTop="30dp"
	></Button>
	
</RelativeLayout>